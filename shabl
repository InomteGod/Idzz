from .. import loader, utils
from asyncio import sleep
import random


class BrawlMod(loader.Module):
    strings = {"name": "BrawlMod"}

    async def client_ready(self, client, db) -> None:
        self.db = db
        self.client = client

    async def Brawl1cmd(self, message):
        """by Inomte :3 Пример ввода: .brawl1 <задержка появления текста в секундах> <шапка>"""
        args = utils.get_args(message)
        if not args:
            self.db.set(self.strings["name"], "state", False)
            await utils.answer(message, "<b>Модуль остановлен!</b>")
            return
        await utils.answer(
            message,
            "<b>Модуль запущен!\n\n"
            "Чтобы его остановить, используй <code>.brawl1</code></b>",
        )
        try:
            time = float(args[0])
        except ValueError:
            return
        text = " ".join(utils.get_args_raw(message).split()[1:])
        shabl = [
"я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ ᴛы ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴇбᴀный и чᴇ ᴛᴇᴨᴇᴩь ᴛы ᴄын ɯᴧюɯий ʍнᴇ ᴄᴋᴀжᴇɯь ᴛᴏ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩидуᴩᴏᴋ ёбᴀнный иди ᴄюдᴀ ᴏᴨяᴛь ᴛᴇбᴇ ᴨиɜды ᴛᴀᴋ дᴀʍ чᴛᴏ нᴇ уйдᴇɯь ᴏᴛᴄюдᴀ жиʙыʍ ","ну я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴛы дᴀʙᴀй ᴛᴀʍ ᴨᴏᴨᴀдᴀй нᴀ ʍᴏй чᴧᴇн ᴏᴨяᴛь жᴇ ᴋᴀᴋ ᴛʙᴏя ʍᴀᴛь ","ᴨᴏᴇбу ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь дᴀжᴇ нᴇ ɜᴀʍᴇчу ᴄын ɯᴧюɯий ᴛы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ничᴛᴏжᴇᴄᴛʙᴏ ёбᴀннᴏᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбу ɜдᴇᴄь ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴛы ёбᴀнный ᴨᴩиʙыᴋᴀй ᴋ ϶ᴛᴏʍу","чᴇ ᴛы нᴇᴄᴇɯь ᴏᴄᴇᴧ ёбᴀнный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙнᴀᴛуᴩᴇ ᴛᴇᴩᴨиᴧᴏид ᴇбᴀный ","я ᴛᴇбᴇ ᴄᴋᴀᴧьᴨ хуᴇʍ ᴄниʍу ᴄын ɯᴀᴧᴀʙы ёбᴀнный ᴛᴇᴩᴨи ʍᴇня","я ᴄʙᴏиʍ чᴧᴇнᴏʍ ᴛᴇбᴇ ᴦᴏᴧᴏʙу ᴨᴩᴏциᴩᴋуᴧиᴩую уᴇбᴀн ёбᴀнный ","ᴛы бᴇɜдᴀᴩнᴏᴄᴛь ёбᴀннᴀя хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛʙᴏи ɜубы нᴇ ʙыбиᴧ","я ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴋᴏᴄᴛи ᴨᴇᴩᴇᴧᴏʍᴀю ʍᴇдᴧᴇнный ᴄынуᴧя ɯᴀᴧᴀʙы ᴛы ","хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛᴇбᴇ чᴇᴧюᴄᴛи нᴇ ʙыбиᴧ ᴏᴨяᴛь ᴄʙᴏиʍ чᴧᴇнᴏʍ ","я ᴛʙᴏᴇ ʍяᴄᴏ ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ёбᴀный ","ᴨᴇᴩᴇᴩᴇжу ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴦᴏᴧᴏʙу нᴀ дʙᴇ чᴀᴄᴛи","я убью ᴛᴇбя ʙᴇдь ᴛʙᴏя жиɜнь хуйня ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь ᴨᴇᴩᴇᴧᴏʍᴀю ᴛы ᴄынᴏᴋ ɯᴧюхи ","ᴨᴀᴋᴧи ᴛᴇбᴇ ᴨᴇᴩᴇᴩᴇжу ᴄын ɯᴀᴧᴀʙы ᴛы ","ᴨᴀᴛᴧы ᴛʙᴏи ʙыᴩʙу ᴄын чᴀйниᴋᴀ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄᴀʍᴏʙᴀᴩ ᴀᴛᴩᴏɸиᴩᴏʙᴀнный дᴀʙᴀй хуй ᴄᴏᴄи","ᴋу я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ я дуʍᴀю чᴛᴏ ᴛы ʙᴄᴇ ᴇщᴇ ᴦубᴋᴀ ᴛᴀᴋ чᴛᴏ ничᴇ ᴄᴛᴩᴀɯнᴏᴦᴏ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴏнᴀ ᴋᴀᴋ ᴇбучᴀя бубыᴧдᴀ ʍнᴇ чᴧᴇн ᴄᴏᴄᴀᴧᴀ ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь хуᴇʍ ᴄᴨᴩᴇᴄую ᴦᴏбᴧин ᴇбучий ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴀᴇбу ᴄын ɯᴀᴧᴀʙы ᴛы ᴦᴧуᴨы нᴀхуй","дᴀʙᴀй ʍнᴇ хуй ᴄᴏᴄи чучᴇᴧᴏ ᴇбᴀнᴏᴇ ᴨᴏᴋᴀ я ᴛᴇбᴇ ᴏᴨяᴛь ᴋᴧᴇɯни нᴇ ᴨᴇᴩᴇᴧᴏʍᴀᴧ ","ᴄᴧыɯиɯь ʍᴇня ᴩᴀᴋᴏᴏбᴩᴀɜный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴀдᴀᴧьщиᴋ ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴀᴇбᴀᴧ ᴛы  ᴄын ɯᴀᴧы ᴛы нᴀ ᴋᴏᴧᴇни дᴀʙᴀй жиʙᴏ ","я ᴛʙᴏю ʍᴀᴛь буду хуᴇʍ ᴇбᴀᴛь ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴛы дᴀʙᴀй ᴛᴀʍ ʍнᴇ хуй ᴄᴏᴄᴇ ᴛᴇᴧᴋᴀ  ᴇбᴀнᴀя ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ жᴏᴨу ᴨᴩᴀʙᴏᴄᴧᴀʙныʍ ᴋᴩᴇᴄᴛᴏʍ ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ","ᴄᴏᴄᴇɯь","уᴄᴏᴄи","ʙᴄᴏᴄи","нᴀᴄᴏᴄи","ᴨᴏᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ ᴋᴀᴋ бычᴏᴋ ","бычᴋуᴇɯь ʍᴏй хуй ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ᴄын ᴦᴏʙнищᴀ ","ᴄᴏᴄи ᴄын ᴦᴏʙнᴀ ᴇбᴀный ","чᴇ ᴄдыхᴀᴇɯь ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","бᴧя ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙᴇᴄь ᴛᴇᴧᴇᴦᴩᴀʍ ᴏᴛʙᴇчᴀю","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙ дᴀᴩᴋнᴇᴛᴇ ","я ʙнᴀᴛуᴩᴇ ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴏᴨьᴇ нᴀᴄᴀдиᴧ ","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ ᴨᴩᴏᴋᴏᴧᴏᴧ ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ нᴀ ᴄʙᴏй хуй ʙᴄᴀжу ","ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴄᴧыɯ хуй ᴄᴏᴄи ᴄын ɯᴀᴧᴀʙы ёбнуᴛый","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ нᴀʙᴏɜниᴋ ᴇбᴀный","жᴩи хуй ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ᴨᴏниʍᴀᴇɯь нᴇᴛ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴇᴄᴧи ᴛы нᴇ ᴨᴏняᴧ","ᴩуᴋи ᴛᴇбᴇ ᴨᴇᴩᴇᴧᴏʍᴀю ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ʍнᴇ ᴋᴩиʙᴏᴩуᴋий ᴄын ɯᴀᴧᴀʙы","ʍᴇдᴧᴇнный ᴄын ɯᴀᴧᴀʙы ᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ хуй иɜᴇ ","ᴄᴏᴄᴇɯь ʍᴏю хуяᴋу ᴧяʙᴩᴀ ᴇбᴀнᴀя","ᴄᴧыɯ чᴧᴇн ᴄᴏᴄи ʍᴏй","я ᴛʙᴏй ᴋиɯᴇчниᴋ ᴇбᴀᴧ ","ᴨᴏʙᴇɯу ᴛᴇбя нᴀ ᴛʙᴏих ᴋиɯᴋᴀх ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь бᴇнɜᴏᴨиᴧᴏй ᴇбᴀᴧ ","ᴨиɜду ᴛʙᴏᴇй ʍᴀʍᴀɯи нᴏжᴏʍ ᴨᴏᴩᴇɜᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴩᴇᴄᴛᴇ ᴩᴀᴄᴨяᴧ ᴄын ɯᴀᴧᴀʙы ᴛы ","хуй ᴄᴏᴄи ᴄын ᴛуᴨᴏᴩыᴧᴏй ɯᴀᴧᴀʙы","я ᴛʙᴏй ᴩᴏᴛ ᴇбᴀᴧ хуᴇᴄᴏᴄищᴇ ᴄᴧᴀбᴇйɯᴇᴇ ","ʙᴏ ʙᴄᴇʍ ᴛᴇᴧᴇᴦᴩᴀʍᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","ʙᴇɜдᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ дᴀжᴇ ᴨᴏд ʙᴏдᴏй ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ ᴏᴦнᴇ ","ᴇбу я ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ ɯᴀᴧᴀʙу ʙᴄᴇ жᴇ ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь и нᴇ ᴏᴛʙᴇчᴀᴇɯь ниᴋᴏʍу","ᴛʙᴏя ʍᴀᴛь ʍᴏй хуй ʍуᴄᴏᴧиᴛ ʙнᴀᴛуᴩᴇ ","ᴋᴀᴋ бы ᴛы нᴇ ᴦᴏʙᴏᴩиᴧ чᴛᴏ ᴛы нᴇ ᴄын ɯᴀᴧᴀʙы ʙᴄᴇ ᴩᴀʙнᴏ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь ɯᴀᴧᴀʙу ɜдᴇᴄь хуᴇʍ ᴨᴩᴏбиᴧ ᴨᴩиᴋинь ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ ᴛуᴛ хуᴇʍ ᴄʙᴏиʍ ᴧиᴋʙидиᴩᴏʙᴀᴧ ","ᴛʙᴏя ʍᴀᴛь ʍнᴇ ᴨᴏᴄᴛᴏяннᴏ хуй ᴄᴏᴄᴀᴧᴀ и чᴛᴏ ᴛы ʍнᴇ ᴄдᴇᴧᴀᴇɯь?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ᴛы чᴇ ᴇбᴀнуᴧᴄя ᴛᴀʍ ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴀᴋ дᴏᴧᴦᴏ...","ɜᴀᴇбᴀᴧ ʍнᴇ ᴄᴏᴄᴀᴛь ᴇбᴧᴀн...","ᴄᴏᴄи ʍнᴇ хуй ᴄын ɯᴀᴧᴀʙы ᴛуᴨᴏᴩыᴧьниᴋᴏʙый ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨиɜдᴀᴋᴩяᴋ ᴇбᴀный","хᴩяᴋ ᴄᴏᴄи хуй ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩᴏᴄᴛиᴛуᴛᴋу ","я ᴛʙᴏю ʍᴀᴛь ɯᴏᴧᴏʙу нᴀ ᴄʙᴏᴇʍ чᴧᴇнᴇ ᴨᴏхᴏᴩᴏниᴧ и ʙᴄᴇ","ᴀ ᴛы ʍнᴇ чᴧᴇн ᴄᴏᴄᴇɯь и нᴇ ᴄᴛᴇᴄняᴇɯьᴄя ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь дᴀжᴇ нᴇ ᴋᴩᴀᴄнᴇᴇɯь ","ну и чᴇ ᴛы ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴏ нᴀчᴀᴧ ᴄын ɯᴀᴧᴀʙы","ᴄын ɯᴀᴧᴀʙы ᴄуᴛᴏчный нᴇ ᴄдᴏхни ᴛуᴛ ","ᴛʙᴏя ʍᴀᴛь ɯᴧюхᴀ ᴇбᴀнᴀя ʙнᴀᴛуᴩᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ и чᴇ ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴏᴋᴩыᴧ ","ᴄᴧыɯ ᴨиɜдᴀᴋᴩыᴧ ᴇбᴀный ᴄᴏᴄи ","ᴄᴧыɯ чᴇᴩʙяᴋ ᴄᴏᴄни ʍнᴇ хуй ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ хуᴇʍ динᴀʍᴧю ᴋᴀᴋ ɯᴀᴧᴀʙу","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ буду иɜбиʙᴀᴛь ᴄын ɯᴀᴧᴀʙы ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь дᴏ ᴨᴏᴧуᴄʍᴇᴩᴛи ᴛуᴛ хуᴇʍ иɜбиʙᴀᴛь буду ᴨᴏняᴧ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴏᴋᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴀᴧ ","ᴄᴧыɯ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴦᴏʙнᴏᴇдя ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ ᴛы ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴇбᴀный и чᴇ ᴛᴇᴨᴇᴩь ᴛы ᴄын ɯᴧюɯий ʍнᴇ ᴄᴋᴀжᴇɯь ᴛᴏ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩидуᴩᴏᴋ ёбᴀнный иди ᴄюдᴀ ᴏᴨяᴛь ᴛᴇбᴇ ᴨиɜды ᴛᴀᴋ дᴀʍ чᴛᴏ нᴇ уйдᴇɯь ᴏᴛᴄюдᴀ жиʙыʍ ","ну я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴛы дᴀʙᴀй ᴛᴀʍ ᴨᴏᴨᴀдᴀй нᴀ ʍᴏй чᴧᴇн ᴏᴨяᴛь жᴇ ᴋᴀᴋ ᴛʙᴏя ʍᴀᴛь ","ᴨᴏᴇбу ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь дᴀжᴇ нᴇ ɜᴀʍᴇчу ᴄын ɯᴧюɯий ᴛы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ничᴛᴏжᴇᴄᴛʙᴏ ёбᴀннᴏᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбу ɜдᴇᴄь ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴛы ёбᴀнный ᴨᴩиʙыᴋᴀй ᴋ ϶ᴛᴏʍу","чᴇ ᴛы нᴇᴄᴇɯь ᴏᴄᴇᴧ ёбᴀнный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙнᴀᴛуᴩᴇ ᴛᴇᴩᴨиᴧᴏид ᴇбᴀный ","я ᴛᴇбᴇ ᴄᴋᴀᴧьᴨ хуᴇʍ ᴄниʍу ᴄын ɯᴀᴧᴀʙы ёбᴀнный ᴛᴇᴩᴨи ʍᴇня","я ᴄʙᴏиʍ чᴧᴇнᴏʍ ᴛᴇбᴇ ᴦᴏᴧᴏʙу ᴨᴩᴏциᴩᴋуᴧиᴩую уᴇбᴀн ёбᴀнный ","ᴛы бᴇɜдᴀᴩнᴏᴄᴛь ёбᴀннᴀя хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛʙᴏи ɜубы нᴇ ʙыбиᴧ","я ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴋᴏᴄᴛи ᴨᴇᴩᴇᴧᴏʍᴀю ʍᴇдᴧᴇнный ᴄынуᴧя ɯᴀᴧᴀʙы ᴛы ","хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛᴇбᴇ чᴇᴧюᴄᴛи нᴇ ʙыбиᴧ ᴏᴨяᴛь ᴄʙᴏиʍ чᴧᴇнᴏʍ ","я ᴛʙᴏᴇ ʍяᴄᴏ ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ёбᴀный ","ᴨᴇᴩᴇᴩᴇжу ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴦᴏᴧᴏʙу нᴀ дʙᴇ чᴀᴄᴛи","я убью ᴛᴇбя ʙᴇдь ᴛʙᴏя жиɜнь хуйня ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь ᴨᴇᴩᴇᴧᴏʍᴀю ᴛы ᴄынᴏᴋ ɯᴧюхи ","ᴨᴀᴋᴧи ᴛᴇбᴇ ᴨᴇᴩᴇᴩᴇжу ᴄын ɯᴀᴧᴀʙы ᴛы ","ᴨᴀᴛᴧы ᴛʙᴏи ʙыᴩʙу ᴄын чᴀйниᴋᴀ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄᴀʍᴏʙᴀᴩ ᴀᴛᴩᴏɸиᴩᴏʙᴀнный дᴀʙᴀй хуй ᴄᴏᴄи","ᴋу я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ я дуʍᴀю чᴛᴏ ᴛы ʙᴄᴇ ᴇщᴇ ᴦубᴋᴀ ᴛᴀᴋ чᴛᴏ ничᴇ ᴄᴛᴩᴀɯнᴏᴦᴏ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴏнᴀ ᴋᴀᴋ ᴇбучᴀя бубыᴧдᴀ ʍнᴇ чᴧᴇн ᴄᴏᴄᴀᴧᴀ ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь хуᴇʍ ᴄᴨᴩᴇᴄую ᴦᴏбᴧин ᴇбучий ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴀᴇбу ᴄын ɯᴀᴧᴀʙы ᴛы ᴦᴧуᴨы нᴀхуй","дᴀʙᴀй ʍнᴇ хуй ᴄᴏᴄи чучᴇᴧᴏ ᴇбᴀнᴏᴇ ᴨᴏᴋᴀ я ᴛᴇбᴇ ᴏᴨяᴛь ᴋᴧᴇɯни нᴇ ᴨᴇᴩᴇᴧᴏʍᴀᴧ ","ᴄᴧыɯиɯь ʍᴇня ᴩᴀᴋᴏᴏбᴩᴀɜный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴀдᴀᴧьщиᴋ ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴀᴇбᴀᴧ ᴛы  ᴄын ɯᴀᴧы ᴛы нᴀ ᴋᴏᴧᴇни дᴀʙᴀй жиʙᴏ ","я ᴛʙᴏю ʍᴀᴛь буду хуᴇʍ ᴇбᴀᴛь ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴛы дᴀʙᴀй ᴛᴀʍ ʍнᴇ хуй ᴄᴏᴄᴇ ᴛᴇᴧᴋᴀ  ᴇбᴀнᴀя ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ жᴏᴨу ᴨᴩᴀʙᴏᴄᴧᴀʙныʍ ᴋᴩᴇᴄᴛᴏʍ ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ","ᴄᴏᴄᴇɯь","уᴄᴏᴄи","ʙᴄᴏᴄи","нᴀᴄᴏᴄи","ᴨᴏᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ ᴋᴀᴋ бычᴏᴋ ","бычᴋуᴇɯь ʍᴏй хуй ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ᴄын ᴦᴏʙнищᴀ ","ᴄᴏᴄи ᴄын ᴦᴏʙнᴀ ᴇбᴀный ","чᴇ ᴄдыхᴀᴇɯь ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","бᴧя ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙᴇᴄь ᴛᴇᴧᴇᴦᴩᴀʍ ᴏᴛʙᴇчᴀю","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙ дᴀᴩᴋнᴇᴛᴇ ","я ʙнᴀᴛуᴩᴇ ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴏᴨьᴇ нᴀᴄᴀдиᴧ ","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ ᴨᴩᴏᴋᴏᴧᴏᴧ ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ нᴀ ᴄʙᴏй хуй ʙᴄᴀжу ","ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴄᴧыɯ хуй ᴄᴏᴄи ᴄын ɯᴀᴧᴀʙы ёбнуᴛый","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ нᴀʙᴏɜниᴋ ᴇбᴀный","жᴩи хуй ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ᴨᴏниʍᴀᴇɯь нᴇᴛ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴇᴄᴧи ᴛы нᴇ ᴨᴏняᴧ","ᴩуᴋи ᴛᴇбᴇ ᴨᴇᴩᴇᴧᴏʍᴀю ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ʍнᴇ ᴋᴩиʙᴏᴩуᴋий ᴄын ɯᴀᴧᴀʙы","ʍᴇдᴧᴇнный ᴄын ɯᴀᴧᴀʙы ᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ хуй иɜᴇ ","ᴄᴏᴄᴇɯь ʍᴏю хуяᴋу ᴧяʙᴩᴀ ᴇбᴀнᴀя","ᴄᴧыɯ чᴧᴇн ᴄᴏᴄи ʍᴏй","я ᴛʙᴏй ᴋиɯᴇчниᴋ ᴇбᴀᴧ ","ᴨᴏʙᴇɯу ᴛᴇбя нᴀ ᴛʙᴏих ᴋиɯᴋᴀх ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь бᴇнɜᴏᴨиᴧᴏй ᴇбᴀᴧ ","ᴨиɜду ᴛʙᴏᴇй ʍᴀʍᴀɯи нᴏжᴏʍ ᴨᴏᴩᴇɜᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴩᴇᴄᴛᴇ ᴩᴀᴄᴨяᴧ ᴄын ɯᴀᴧᴀʙы ᴛы ","хуй ᴄᴏᴄи ᴄын ᴛуᴨᴏᴩыᴧᴏй ɯᴀᴧᴀʙы","я ᴛʙᴏй ᴩᴏᴛ ᴇбᴀᴧ хуᴇᴄᴏᴄищᴇ ᴄᴧᴀбᴇйɯᴇᴇ ","ʙᴏ ʙᴄᴇʍ ᴛᴇᴧᴇᴦᴩᴀʍᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","ʙᴇɜдᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ дᴀжᴇ ᴨᴏд ʙᴏдᴏй ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ ᴏᴦнᴇ ","ᴇбу я ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ ɯᴀᴧᴀʙу ʙᴄᴇ жᴇ ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь и нᴇ ᴏᴛʙᴇчᴀᴇɯь ниᴋᴏʍу","ᴛʙᴏя ʍᴀᴛь ʍᴏй хуй ʍуᴄᴏᴧиᴛ ʙнᴀᴛуᴩᴇ ","ᴋᴀᴋ бы ᴛы нᴇ ᴦᴏʙᴏᴩиᴧ чᴛᴏ ᴛы нᴇ ᴄын ɯᴀᴧᴀʙы ʙᴄᴇ ᴩᴀʙнᴏ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь ɯᴀᴧᴀʙу ɜдᴇᴄь хуᴇʍ ᴨᴩᴏбиᴧ ᴨᴩиᴋинь ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ ᴛуᴛ хуᴇʍ ᴄʙᴏиʍ ᴧиᴋʙидиᴩᴏʙᴀᴧ ","ᴛʙᴏя ʍᴀᴛь ʍнᴇ ᴨᴏᴄᴛᴏяннᴏ хуй ᴄᴏᴄᴀᴧᴀ и чᴛᴏ ᴛы ʍнᴇ ᴄдᴇᴧᴀᴇɯь?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ᴛы чᴇ ᴇбᴀнуᴧᴄя ᴛᴀʍ ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴀᴋ дᴏᴧᴦᴏ...","ɜᴀᴇбᴀᴧ ʍнᴇ ᴄᴏᴄᴀᴛь ᴇбᴧᴀн...","ᴄᴏᴄи ʍнᴇ хуй ᴄын ɯᴀᴧᴀʙы ᴛуᴨᴏᴩыᴧьниᴋᴏʙый ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨиɜдᴀᴋᴩяᴋ ᴇбᴀный","хᴩяᴋ ᴄᴏᴄи хуй ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩᴏᴄᴛиᴛуᴛᴋу ","я ᴛʙᴏю ʍᴀᴛь ɯᴏᴧᴏʙу нᴀ ᴄʙᴏᴇʍ чᴧᴇнᴇ ᴨᴏхᴏᴩᴏниᴧ и ʙᴄᴇ","ᴀ ᴛы ʍнᴇ чᴧᴇн ᴄᴏᴄᴇɯь и нᴇ ᴄᴛᴇᴄняᴇɯьᴄя ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь дᴀжᴇ нᴇ ᴋᴩᴀᴄнᴇᴇɯь ","ну и чᴇ ᴛы ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴏ нᴀчᴀᴧ ᴄын ɯᴀᴧᴀʙы","ᴄын ɯᴀᴧᴀʙы ᴄуᴛᴏчный нᴇ ᴄдᴏхни ᴛуᴛ ","ᴛʙᴏя ʍᴀᴛь ɯᴧюхᴀ ᴇбᴀнᴀя ʙнᴀᴛуᴩᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ и чᴇ ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴏᴋᴩыᴧ ","ᴄᴧыɯ ᴨиɜдᴀᴋᴩыᴧ ᴇбᴀный ᴄᴏᴄи ","ᴄᴧыɯ чᴇᴩʙяᴋ ᴄᴏᴄни ʍнᴇ хуй ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ хуᴇʍ динᴀʍᴧю ᴋᴀᴋ ɯᴀᴧᴀʙу","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ буду иɜбиʙᴀᴛь ᴄын ɯᴀᴧᴀʙы ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь дᴏ ᴨᴏᴧуᴄʍᴇᴩᴛи ᴛуᴛ хуᴇʍ иɜбиʙᴀᴛь буду ᴨᴏняᴧ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴏᴋᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴀᴧ ","ᴄᴧыɯ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴦᴏʙнᴏᴇдя ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ ᴛы ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴇбᴀный и чᴇ ᴛᴇᴨᴇᴩь ᴛы ᴄын ɯᴧюɯий ʍнᴇ ᴄᴋᴀжᴇɯь ᴛᴏ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩидуᴩᴏᴋ ёбᴀнный иди ᴄюдᴀ ᴏᴨяᴛь ᴛᴇбᴇ ᴨиɜды ᴛᴀᴋ дᴀʍ чᴛᴏ нᴇ уйдᴇɯь ᴏᴛᴄюдᴀ жиʙыʍ ","ну я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴛы дᴀʙᴀй ᴛᴀʍ ᴨᴏᴨᴀдᴀй нᴀ ʍᴏй чᴧᴇн ᴏᴨяᴛь жᴇ ᴋᴀᴋ ᴛʙᴏя ʍᴀᴛь ","ᴨᴏᴇбу ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь дᴀжᴇ нᴇ ɜᴀʍᴇчу ᴄын ɯᴧюɯий ᴛы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ничᴛᴏжᴇᴄᴛʙᴏ ёбᴀннᴏᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбу ɜдᴇᴄь ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴛы ёбᴀнный ᴨᴩиʙыᴋᴀй ᴋ ϶ᴛᴏʍу","чᴇ ᴛы нᴇᴄᴇɯь ᴏᴄᴇᴧ ёбᴀнный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙнᴀᴛуᴩᴇ ᴛᴇᴩᴨиᴧᴏид ᴇбᴀный ","я ᴛᴇбᴇ ᴄᴋᴀᴧьᴨ хуᴇʍ ᴄниʍу ᴄын ɯᴀᴧᴀʙы ёбᴀнный ᴛᴇᴩᴨи ʍᴇня","я ᴄʙᴏиʍ чᴧᴇнᴏʍ ᴛᴇбᴇ ᴦᴏᴧᴏʙу ᴨᴩᴏциᴩᴋуᴧиᴩую уᴇбᴀн ёбᴀнный ","ᴛы бᴇɜдᴀᴩнᴏᴄᴛь ёбᴀннᴀя хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛʙᴏи ɜубы нᴇ ʙыбиᴧ","я ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴋᴏᴄᴛи ᴨᴇᴩᴇᴧᴏʍᴀю ʍᴇдᴧᴇнный ᴄынуᴧя ɯᴀᴧᴀʙы ᴛы ","хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛᴇбᴇ чᴇᴧюᴄᴛи нᴇ ʙыбиᴧ ᴏᴨяᴛь ᴄʙᴏиʍ чᴧᴇнᴏʍ ","я ᴛʙᴏᴇ ʍяᴄᴏ ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ёбᴀный ","ᴨᴇᴩᴇᴩᴇжу ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴦᴏᴧᴏʙу нᴀ дʙᴇ чᴀᴄᴛи","я убью ᴛᴇбя ʙᴇдь ᴛʙᴏя жиɜнь хуйня ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь ᴨᴇᴩᴇᴧᴏʍᴀю ᴛы ᴄынᴏᴋ ɯᴧюхи ","ᴨᴀᴋᴧи ᴛᴇбᴇ ᴨᴇᴩᴇᴩᴇжу ᴄын ɯᴀᴧᴀʙы ᴛы ","ᴨᴀᴛᴧы ᴛʙᴏи ʙыᴩʙу ᴄын чᴀйниᴋᴀ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄᴀʍᴏʙᴀᴩ ᴀᴛᴩᴏɸиᴩᴏʙᴀнный дᴀʙᴀй хуй ᴄᴏᴄи","ᴋу я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ я дуʍᴀю чᴛᴏ ᴛы ʙᴄᴇ ᴇщᴇ ᴦубᴋᴀ ᴛᴀᴋ чᴛᴏ ничᴇ ᴄᴛᴩᴀɯнᴏᴦᴏ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴏнᴀ ᴋᴀᴋ ᴇбучᴀя бубыᴧдᴀ ʍнᴇ чᴧᴇн ᴄᴏᴄᴀᴧᴀ ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь хуᴇʍ ᴄᴨᴩᴇᴄую ᴦᴏбᴧин ᴇбучий ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴀᴇбу ᴄын ɯᴀᴧᴀʙы ᴛы ᴦᴧуᴨы нᴀхуй","дᴀʙᴀй ʍнᴇ хуй ᴄᴏᴄи чучᴇᴧᴏ ᴇбᴀнᴏᴇ ᴨᴏᴋᴀ я ᴛᴇбᴇ ᴏᴨяᴛь ᴋᴧᴇɯни нᴇ ᴨᴇᴩᴇᴧᴏʍᴀᴧ ","ᴄᴧыɯиɯь ʍᴇня ᴩᴀᴋᴏᴏбᴩᴀɜный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴀдᴀᴧьщиᴋ ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴀᴇбᴀᴧ ᴛы  ᴄын ɯᴀᴧы ᴛы нᴀ ᴋᴏᴧᴇни дᴀʙᴀй жиʙᴏ ","я ᴛʙᴏю ʍᴀᴛь буду хуᴇʍ ᴇбᴀᴛь ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴛы дᴀʙᴀй ᴛᴀʍ ʍнᴇ хуй ᴄᴏᴄᴇ ᴛᴇᴧᴋᴀ  ᴇбᴀнᴀя ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ жᴏᴨу ᴨᴩᴀʙᴏᴄᴧᴀʙныʍ ᴋᴩᴇᴄᴛᴏʍ ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ","ᴄᴏᴄᴇɯь","уᴄᴏᴄи","ʙᴄᴏᴄи","нᴀᴄᴏᴄи","ᴨᴏᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ ᴋᴀᴋ бычᴏᴋ ","бычᴋуᴇɯь ʍᴏй хуй ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ᴄын ᴦᴏʙнищᴀ ","ᴄᴏᴄи ᴄын ᴦᴏʙнᴀ ᴇбᴀный ","чᴇ ᴄдыхᴀᴇɯь ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","бᴧя ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙᴇᴄь ᴛᴇᴧᴇᴦᴩᴀʍ ᴏᴛʙᴇчᴀю","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙ дᴀᴩᴋнᴇᴛᴇ ","я ʙнᴀᴛуᴩᴇ ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴏᴨьᴇ нᴀᴄᴀдиᴧ ","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ ᴨᴩᴏᴋᴏᴧᴏᴧ ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ нᴀ ᴄʙᴏй хуй ʙᴄᴀжу ","ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴄᴧыɯ хуй ᴄᴏᴄи ᴄын ɯᴀᴧᴀʙы ёбнуᴛый","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ нᴀʙᴏɜниᴋ ᴇбᴀный","жᴩи хуй ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ᴨᴏниʍᴀᴇɯь нᴇᴛ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴇᴄᴧи ᴛы нᴇ ᴨᴏняᴧ","ᴩуᴋи ᴛᴇбᴇ ᴨᴇᴩᴇᴧᴏʍᴀю ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ʍнᴇ ᴋᴩиʙᴏᴩуᴋий ᴄын ɯᴀᴧᴀʙы","ʍᴇдᴧᴇнный ᴄын ɯᴀᴧᴀʙы ᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ хуй иɜᴇ ","ᴄᴏᴄᴇɯь ʍᴏю хуяᴋу ᴧяʙᴩᴀ ᴇбᴀнᴀя","ᴄᴧыɯ чᴧᴇн ᴄᴏᴄи ʍᴏй","я ᴛʙᴏй ᴋиɯᴇчниᴋ ᴇбᴀᴧ ","ᴨᴏʙᴇɯу ᴛᴇбя нᴀ ᴛʙᴏих ᴋиɯᴋᴀх ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь бᴇнɜᴏᴨиᴧᴏй ᴇбᴀᴧ ","ᴨиɜду ᴛʙᴏᴇй ʍᴀʍᴀɯи нᴏжᴏʍ ᴨᴏᴩᴇɜᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴩᴇᴄᴛᴇ ᴩᴀᴄᴨяᴧ ᴄын ɯᴀᴧᴀʙы ᴛы ","хуй ᴄᴏᴄи ᴄын ᴛуᴨᴏᴩыᴧᴏй ɯᴀᴧᴀʙы","я ᴛʙᴏй ᴩᴏᴛ ᴇбᴀᴧ хуᴇᴄᴏᴄищᴇ ᴄᴧᴀбᴇйɯᴇᴇ ","ʙᴏ ʙᴄᴇʍ ᴛᴇᴧᴇᴦᴩᴀʍᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","ʙᴇɜдᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ дᴀжᴇ ᴨᴏд ʙᴏдᴏй ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ ᴏᴦнᴇ ","ᴇбу я ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ ɯᴀᴧᴀʙу ʙᴄᴇ жᴇ ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь и нᴇ ᴏᴛʙᴇчᴀᴇɯь ниᴋᴏʍу","ᴛʙᴏя ʍᴀᴛь ʍᴏй хуй ʍуᴄᴏᴧиᴛ ʙнᴀᴛуᴩᴇ ","ᴋᴀᴋ бы ᴛы нᴇ ᴦᴏʙᴏᴩиᴧ чᴛᴏ ᴛы нᴇ ᴄын ɯᴀᴧᴀʙы ʙᴄᴇ ᴩᴀʙнᴏ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь ɯᴀᴧᴀʙу ɜдᴇᴄь хуᴇʍ ᴨᴩᴏбиᴧ ᴨᴩиᴋинь ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ ᴛуᴛ хуᴇʍ ᴄʙᴏиʍ ᴧиᴋʙидиᴩᴏʙᴀᴧ ","ᴛʙᴏя ʍᴀᴛь ʍнᴇ ᴨᴏᴄᴛᴏяннᴏ хуй ᴄᴏᴄᴀᴧᴀ и чᴛᴏ ᴛы ʍнᴇ ᴄдᴇᴧᴀᴇɯь?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ᴛы чᴇ ᴇбᴀнуᴧᴄя ᴛᴀʍ ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴀᴋ дᴏᴧᴦᴏ...","ɜᴀᴇбᴀᴧ ʍнᴇ ᴄᴏᴄᴀᴛь ᴇбᴧᴀн...","ᴄᴏᴄи ʍнᴇ хуй ᴄын ɯᴀᴧᴀʙы ᴛуᴨᴏᴩыᴧьниᴋᴏʙый ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨиɜдᴀᴋᴩяᴋ ᴇбᴀный","хᴩяᴋ ᴄᴏᴄи хуй ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩᴏᴄᴛиᴛуᴛᴋу ","я ᴛʙᴏю ʍᴀᴛь ɯᴏᴧᴏʙу нᴀ ᴄʙᴏᴇʍ чᴧᴇнᴇ ᴨᴏхᴏᴩᴏниᴧ и ʙᴄᴇ","ᴀ ᴛы ʍнᴇ чᴧᴇн ᴄᴏᴄᴇɯь и нᴇ ᴄᴛᴇᴄняᴇɯьᴄя ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь дᴀжᴇ нᴇ ᴋᴩᴀᴄнᴇᴇɯь ","ну и чᴇ ᴛы ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴏ нᴀчᴀᴧ ᴄын ɯᴀᴧᴀʙы","ᴄын ɯᴀᴧᴀʙы ᴄуᴛᴏчный нᴇ ᴄдᴏхни ᴛуᴛ ","ᴛʙᴏя ʍᴀᴛь ɯᴧюхᴀ ᴇбᴀнᴀя ʙнᴀᴛуᴩᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ и чᴇ ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴏᴋᴩыᴧ ","ᴄᴧыɯ ᴨиɜдᴀᴋᴩыᴧ ᴇбᴀный ᴄᴏᴄи ","ᴄᴧыɯ чᴇᴩʙяᴋ ᴄᴏᴄни ʍнᴇ хуй ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ хуᴇʍ динᴀʍᴧю ᴋᴀᴋ ɯᴀᴧᴀʙу","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ буду иɜбиʙᴀᴛь ᴄын ɯᴀᴧᴀʙы ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь дᴏ ᴨᴏᴧуᴄʍᴇᴩᴛи ᴛуᴛ хуᴇʍ иɜбиʙᴀᴛь буду ᴨᴏняᴧ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴏᴋᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴀᴧ ","ᴄᴧыɯ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴦᴏʙнᴏᴇдя ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ ᴛы ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴇбᴀный и чᴇ ᴛᴇᴨᴇᴩь ᴛы ᴄын ɯᴧюɯий ʍнᴇ ᴄᴋᴀжᴇɯь ᴛᴏ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩидуᴩᴏᴋ ёбᴀнный иди ᴄюдᴀ ᴏᴨяᴛь ᴛᴇбᴇ ᴨиɜды ᴛᴀᴋ дᴀʍ чᴛᴏ нᴇ уйдᴇɯь ᴏᴛᴄюдᴀ жиʙыʍ ","ну я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴛы дᴀʙᴀй ᴛᴀʍ ᴨᴏᴨᴀдᴀй нᴀ ʍᴏй чᴧᴇн ᴏᴨяᴛь жᴇ ᴋᴀᴋ ᴛʙᴏя ʍᴀᴛь ","ᴨᴏᴇбу ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь дᴀжᴇ нᴇ ɜᴀʍᴇчу ᴄын ɯᴧюɯий ᴛы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ничᴛᴏжᴇᴄᴛʙᴏ ёбᴀннᴏᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбу ɜдᴇᴄь ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴛы ёбᴀнный ᴨᴩиʙыᴋᴀй ᴋ ϶ᴛᴏʍу","чᴇ ᴛы нᴇᴄᴇɯь ᴏᴄᴇᴧ ёбᴀнный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙнᴀᴛуᴩᴇ ᴛᴇᴩᴨиᴧᴏид ᴇбᴀный ","я ᴛᴇбᴇ ᴄᴋᴀᴧьᴨ хуᴇʍ ᴄниʍу ᴄын ɯᴀᴧᴀʙы ёбᴀнный ᴛᴇᴩᴨи ʍᴇня","я ᴄʙᴏиʍ чᴧᴇнᴏʍ ᴛᴇбᴇ ᴦᴏᴧᴏʙу ᴨᴩᴏциᴩᴋуᴧиᴩую уᴇбᴀн ёбᴀнный ","ᴛы бᴇɜдᴀᴩнᴏᴄᴛь ёбᴀннᴀя хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛʙᴏи ɜубы нᴇ ʙыбиᴧ","я ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴋᴏᴄᴛи ᴨᴇᴩᴇᴧᴏʍᴀю ʍᴇдᴧᴇнный ᴄынуᴧя ɯᴀᴧᴀʙы ᴛы ","хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛᴇбᴇ чᴇᴧюᴄᴛи нᴇ ʙыбиᴧ ᴏᴨяᴛь ᴄʙᴏиʍ чᴧᴇнᴏʍ ","я ᴛʙᴏᴇ ʍяᴄᴏ ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ёбᴀный ","ᴨᴇᴩᴇᴩᴇжу ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴦᴏᴧᴏʙу нᴀ дʙᴇ чᴀᴄᴛи","я убью ᴛᴇбя ʙᴇдь ᴛʙᴏя жиɜнь хуйня ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь ᴨᴇᴩᴇᴧᴏʍᴀю ᴛы ᴄынᴏᴋ ɯᴧюхи ","ᴨᴀᴋᴧи ᴛᴇбᴇ ᴨᴇᴩᴇᴩᴇжу ᴄын ɯᴀᴧᴀʙы ᴛы ","ᴨᴀᴛᴧы ᴛʙᴏи ʙыᴩʙу ᴄын чᴀйниᴋᴀ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄᴀʍᴏʙᴀᴩ ᴀᴛᴩᴏɸиᴩᴏʙᴀнный дᴀʙᴀй хуй ᴄᴏᴄи","ᴋу я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ я дуʍᴀю чᴛᴏ ᴛы ʙᴄᴇ ᴇщᴇ ᴦубᴋᴀ ᴛᴀᴋ чᴛᴏ ничᴇ ᴄᴛᴩᴀɯнᴏᴦᴏ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴏнᴀ ᴋᴀᴋ ᴇбучᴀя бубыᴧдᴀ ʍнᴇ чᴧᴇн ᴄᴏᴄᴀᴧᴀ ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь хуᴇʍ ᴄᴨᴩᴇᴄую ᴦᴏбᴧин ᴇбучий ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴀᴇбу ᴄын ɯᴀᴧᴀʙы ᴛы ᴦᴧуᴨы нᴀхуй","дᴀʙᴀй ʍнᴇ хуй ᴄᴏᴄи чучᴇᴧᴏ ᴇбᴀнᴏᴇ ᴨᴏᴋᴀ я ᴛᴇбᴇ ᴏᴨяᴛь ᴋᴧᴇɯни нᴇ ᴨᴇᴩᴇᴧᴏʍᴀᴧ ","ᴄᴧыɯиɯь ʍᴇня ᴩᴀᴋᴏᴏбᴩᴀɜный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴀдᴀᴧьщиᴋ ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴀᴇбᴀᴧ ᴛы  ᴄын ɯᴀᴧы ᴛы нᴀ ᴋᴏᴧᴇни дᴀʙᴀй жиʙᴏ ","я ᴛʙᴏю ʍᴀᴛь буду хуᴇʍ ᴇбᴀᴛь ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴛы дᴀʙᴀй ᴛᴀʍ ʍнᴇ хуй ᴄᴏᴄᴇ ᴛᴇᴧᴋᴀ  ᴇбᴀнᴀя ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ жᴏᴨу ᴨᴩᴀʙᴏᴄᴧᴀʙныʍ ᴋᴩᴇᴄᴛᴏʍ ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ","ᴄᴏᴄᴇɯь","уᴄᴏᴄи","ʙᴄᴏᴄи","нᴀᴄᴏᴄи","ᴨᴏᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ ᴋᴀᴋ бычᴏᴋ ","бычᴋуᴇɯь ʍᴏй хуй ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ᴄын ᴦᴏʙнищᴀ ","ᴄᴏᴄи ᴄын ᴦᴏʙнᴀ ᴇбᴀный ","чᴇ ᴄдыхᴀᴇɯь ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","бᴧя ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙᴇᴄь ᴛᴇᴧᴇᴦᴩᴀʍ ᴏᴛʙᴇчᴀю","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙ дᴀᴩᴋнᴇᴛᴇ ","я ʙнᴀᴛуᴩᴇ ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴏᴨьᴇ нᴀᴄᴀдиᴧ ","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ ᴨᴩᴏᴋᴏᴧᴏᴧ ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ нᴀ ᴄʙᴏй хуй ʙᴄᴀжу ","ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴄᴧыɯ хуй ᴄᴏᴄи ᴄын ɯᴀᴧᴀʙы ёбнуᴛый","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ нᴀʙᴏɜниᴋ ᴇбᴀный","жᴩи хуй ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ᴨᴏниʍᴀᴇɯь нᴇᴛ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴇᴄᴧи ᴛы нᴇ ᴨᴏняᴧ","ᴩуᴋи ᴛᴇбᴇ ᴨᴇᴩᴇᴧᴏʍᴀю ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ʍнᴇ ᴋᴩиʙᴏᴩуᴋий ᴄын ɯᴀᴧᴀʙы","ʍᴇдᴧᴇнный ᴄын ɯᴀᴧᴀʙы ᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ хуй иɜᴇ ","ᴄᴏᴄᴇɯь ʍᴏю хуяᴋу ᴧяʙᴩᴀ ᴇбᴀнᴀя","ᴄᴧыɯ чᴧᴇн ᴄᴏᴄи ʍᴏй","я ᴛʙᴏй ᴋиɯᴇчниᴋ ᴇбᴀᴧ ","ᴨᴏʙᴇɯу ᴛᴇбя нᴀ ᴛʙᴏих ᴋиɯᴋᴀх ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь бᴇнɜᴏᴨиᴧᴏй ᴇбᴀᴧ ","ᴨиɜду ᴛʙᴏᴇй ʍᴀʍᴀɯи нᴏжᴏʍ ᴨᴏᴩᴇɜᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴩᴇᴄᴛᴇ ᴩᴀᴄᴨяᴧ ᴄын ɯᴀᴧᴀʙы ᴛы ","хуй ᴄᴏᴄи ᴄын ᴛуᴨᴏᴩыᴧᴏй ɯᴀᴧᴀʙы","я ᴛʙᴏй ᴩᴏᴛ ᴇбᴀᴧ хуᴇᴄᴏᴄищᴇ ᴄᴧᴀбᴇйɯᴇᴇ ","ʙᴏ ʙᴄᴇʍ ᴛᴇᴧᴇᴦᴩᴀʍᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","ʙᴇɜдᴇ ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ дᴀжᴇ ᴨᴏд ʙᴏдᴏй ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ ᴏᴦнᴇ ","ᴇбу я ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ ɯᴀᴧᴀʙу ʙᴄᴇ жᴇ ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь и нᴇ ᴏᴛʙᴇчᴀᴇɯь ниᴋᴏʍу","ᴛʙᴏя ʍᴀᴛь ʍᴏй хуй ʍуᴄᴏᴧиᴛ ʙнᴀᴛуᴩᴇ ","ᴋᴀᴋ бы ᴛы нᴇ ᴦᴏʙᴏᴩиᴧ чᴛᴏ ᴛы нᴇ ᴄын ɯᴀᴧᴀʙы ʙᴄᴇ ᴩᴀʙнᴏ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","я ᴛʙᴏю ʍᴀᴛь ɯᴀᴧᴀʙу ɜдᴇᴄь хуᴇʍ ᴨᴩᴏбиᴧ ᴨᴩиᴋинь ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ ᴛуᴛ хуᴇʍ ᴄʙᴏиʍ ᴧиᴋʙидиᴩᴏʙᴀᴧ ","ᴛʙᴏя ʍᴀᴛь ʍнᴇ ᴨᴏᴄᴛᴏяннᴏ хуй ᴄᴏᴄᴀᴧᴀ и чᴛᴏ ᴛы ʍнᴇ ᴄдᴇᴧᴀᴇɯь?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ᴛы чᴇ ᴇбᴀнуᴧᴄя ᴛᴀʍ ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴀᴋ дᴏᴧᴦᴏ...","ɜᴀᴇбᴀᴧ ʍнᴇ ᴄᴏᴄᴀᴛь ᴇбᴧᴀн...","ᴄᴏᴄи ʍнᴇ хуй ᴄын ɯᴀᴧᴀʙы ᴛуᴨᴏᴩыᴧьниᴋᴏʙый ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨиɜдᴀᴋᴩяᴋ ᴇбᴀный","хᴩяᴋ ᴄᴏᴄи хуй ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩᴏᴄᴛиᴛуᴛᴋу ","я ᴛʙᴏю ʍᴀᴛь ɯᴏᴧᴏʙу нᴀ ᴄʙᴏᴇʍ чᴧᴇнᴇ ᴨᴏхᴏᴩᴏниᴧ и ʙᴄᴇ","ᴀ ᴛы ʍнᴇ чᴧᴇн ᴄᴏᴄᴇɯь и нᴇ ᴄᴛᴇᴄняᴇɯьᴄя ","ᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴇɯь дᴀжᴇ нᴇ ᴋᴩᴀᴄнᴇᴇɯь ","ну и чᴇ ᴛы ʍнᴇ ᴄᴏᴄᴀᴛь ᴛᴏ нᴀчᴀᴧ ᴄын ɯᴀᴧᴀʙы","ᴄын ɯᴀᴧᴀʙы ᴄуᴛᴏчный нᴇ ᴄдᴏхни ᴛуᴛ ","ᴛʙᴏя ʍᴀᴛь ɯᴧюхᴀ ᴇбᴀнᴀя ʙнᴀᴛуᴩᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ и чᴇ ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴏᴋᴩыᴧ ","ᴄᴧыɯ ᴨиɜдᴀᴋᴩыᴧ ᴇбᴀный ᴄᴏᴄи ","ᴄᴧыɯ чᴇᴩʙяᴋ ᴄᴏᴄни ʍнᴇ хуй ","я ᴛʙᴏю ʍᴀᴛь ʙнᴀᴛуᴩᴇ хуᴇʍ динᴀʍᴧю ᴋᴀᴋ ɯᴀᴧᴀʙу","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь хуᴇʍ буду иɜбиʙᴀᴛь ᴄын ɯᴀᴧᴀʙы ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь дᴏ ᴨᴏᴧуᴄʍᴇᴩᴛи ᴛуᴛ хуᴇʍ иɜбиʙᴀᴛь буду ᴨᴏняᴧ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴏᴋᴀ ᴛы ʍнᴇ хуй ᴄᴏᴄᴀᴧ ","ᴄᴧыɯ я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴦᴏʙнᴏᴇдя ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ ᴛы ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴇбᴀный и чᴇ ᴛᴇᴨᴇᴩь ᴛы ᴄын ɯᴧюɯий ʍнᴇ ᴄᴋᴀжᴇɯь ᴛᴏ?","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴩидуᴩᴏᴋ ёбᴀнный иди ᴄюдᴀ ᴏᴨяᴛь ᴛᴇбᴇ ᴨиɜды ᴛᴀᴋ дᴀʍ чᴛᴏ нᴇ уйдᴇɯь ᴏᴛᴄюдᴀ жиʙыʍ ","ну я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴛы дᴀʙᴀй ᴛᴀʍ ᴨᴏᴨᴀдᴀй нᴀ ʍᴏй чᴧᴇн ᴏᴨяᴛь жᴇ ᴋᴀᴋ ᴛʙᴏя ʍᴀᴛь ","ᴨᴏᴇбу ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь дᴀжᴇ нᴇ ɜᴀʍᴇчу ᴄын ɯᴧюɯий ᴛы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ничᴛᴏжᴇᴄᴛʙᴏ ёбᴀннᴏᴇ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбу ɜдᴇᴄь ᴄынᴏᴋ ɯᴀᴧᴀʙы ᴛы ёбᴀнный ᴨᴩиʙыᴋᴀй ᴋ ϶ᴛᴏʍу","чᴇ ᴛы нᴇᴄᴇɯь ᴏᴄᴇᴧ ёбᴀнный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙнᴀᴛуᴩᴇ ᴛᴇᴩᴨиᴧᴏид ᴇбᴀный ","я ᴛᴇбᴇ ᴄᴋᴀᴧьᴨ хуᴇʍ ᴄниʍу ᴄын ɯᴀᴧᴀʙы ёбᴀнный ᴛᴇᴩᴨи ʍᴇня","я ᴄʙᴏиʍ чᴧᴇнᴏʍ ᴛᴇбᴇ ᴦᴏᴧᴏʙу ᴨᴩᴏциᴩᴋуᴧиᴩую уᴇбᴀн ёбᴀнный ","ᴛы бᴇɜдᴀᴩнᴏᴄᴛь ёбᴀннᴀя хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛʙᴏи ɜубы нᴇ ʙыбиᴧ","я ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴋᴏᴄᴛи ᴨᴇᴩᴇᴧᴏʍᴀю ʍᴇдᴧᴇнный ᴄынуᴧя ɯᴀᴧᴀʙы ᴛы ","хуй ʍᴏй ᴄᴏᴄи ᴨᴏᴋᴀ я ᴛᴇбᴇ чᴇᴧюᴄᴛи нᴇ ʙыбиᴧ ᴏᴨяᴛь ᴄʙᴏиʍ чᴧᴇнᴏʍ ","я ᴛʙᴏᴇ ʍяᴄᴏ ᴇбᴀᴧ ᴄын ɯᴀᴧᴀʙы ёбᴀный ","ᴨᴇᴩᴇᴩᴇжу ᴛʙᴏᴇй ʍᴀʍᴀɯᴇ ɜдᴇᴄь ᴦᴏᴧᴏʙу нᴀ дʙᴇ чᴀᴄᴛи","я убью ᴛᴇбя ʙᴇдь ᴛʙᴏя жиɜнь хуйня ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь ᴨᴇᴩᴇᴧᴏʍᴀю ᴛы ᴄынᴏᴋ ɯᴧюхи ","ᴨᴀᴋᴧи ᴛᴇбᴇ ᴨᴇᴩᴇᴩᴇжу ᴄын ɯᴀᴧᴀʙы ᴛы ","ᴨᴀᴛᴧы ᴛʙᴏи ʙыᴩʙу ᴄын чᴀйниᴋᴀ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴄᴀʍᴏʙᴀᴩ ᴀᴛᴩᴏɸиᴩᴏʙᴀнный дᴀʙᴀй хуй ᴄᴏᴄи","ᴋу я ᴛʙᴏю ʍᴀᴛь ᴨᴏᴇбᴀᴧ я дуʍᴀю чᴛᴏ ᴛы ʙᴄᴇ ᴇщᴇ ᴦубᴋᴀ ᴛᴀᴋ чᴛᴏ ничᴇ ᴄᴛᴩᴀɯнᴏᴦᴏ ","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴏнᴀ ᴋᴀᴋ ᴇбучᴀя бубыᴧдᴀ ʍнᴇ чᴧᴇн ᴄᴏᴄᴀᴧᴀ ","я ᴛʙᴏю ʍᴀʍᴀɯу ɜдᴇᴄь хуᴇʍ ᴄᴨᴩᴇᴄую ᴦᴏбᴧин ᴇбучий ","я ᴛʙᴏю ʍᴀᴛь хуᴇʍ ᴨᴀᴇбу ᴄын ɯᴀᴧᴀʙы ᴛы ᴦᴧуᴨы нᴀхуй","дᴀʙᴀй ʍнᴇ хуй ᴄᴏᴄи чучᴇᴧᴏ ᴇбᴀнᴏᴇ ᴨᴏᴋᴀ я ᴛᴇбᴇ ᴏᴨяᴛь ᴋᴧᴇɯни нᴇ ᴨᴇᴩᴇᴧᴏʍᴀᴧ ","ᴄᴧыɯиɯь ʍᴇня ᴩᴀᴋᴏᴏбᴩᴀɜный я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ᴨᴀдᴀᴧьщиᴋ ᴛы ᴄуᴋᴀ ","я ᴛʙᴏю ʍᴀᴛь ᴨᴀᴇбᴀᴧ ᴛы  ᴄын ɯᴀᴧы ᴛы нᴀ ᴋᴏᴧᴇни дᴀʙᴀй жиʙᴏ ","я ᴛʙᴏю ʍᴀᴛь буду хуᴇʍ ᴇбᴀᴛь ᴛы ᴄын ɯᴀᴧᴀʙы ᴄᴧᴀбᴇйɯий ","ᴛы дᴀʙᴀй ᴛᴀʍ ʍнᴇ хуй ᴄᴏᴄᴇ ᴛᴇᴧᴋᴀ  ᴇбᴀнᴀя ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ʙ жᴏᴨу ᴨᴩᴀʙᴏᴄᴧᴀʙныʍ ᴋᴩᴇᴄᴛᴏʍ ","ᴇбᴀᴧ ᴛʙᴏю ʍᴀᴛь ᴋᴀᴋ нᴇхуй дᴇᴧᴀᴛь ","ᴄᴏᴄᴇɯь","уᴄᴏᴄи","ʙᴄᴏᴄи","нᴀᴄᴏᴄи","ᴨᴏᴄᴏᴄи","ᴄᴏᴄᴇɯь ʍнᴇ ᴋᴀᴋ бычᴏᴋ ","бычᴋуᴇɯь ʍᴏй хуй ᴄын ɯᴀᴧᴀʙы","ᴏᴛᴄᴏᴄи ᴄын ᴦᴏʙнищᴀ ","ᴄᴏᴄи ᴄын ᴦᴏʙнᴀ ᴇбᴀный ","чᴇ ᴄдыхᴀᴇɯь ᴄын ɯᴀᴧᴀʙы","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ","бᴧя ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙᴇᴄь ᴛᴇᴧᴇᴦᴩᴀʍ ᴏᴛʙᴇчᴀю","я ᴛʙᴏю ʍᴀᴛь ᴇбᴀᴧ ʙ дᴀᴩᴋнᴇᴛᴇ ","я ʙнᴀᴛуᴩᴇ ᴛʙᴏю ʍᴀᴛь нᴀ ᴋᴏᴨьᴇ нᴀᴄᴀдиᴧ ","я ᴛʙᴏю ʍᴀᴛь ɜдᴇᴄь