import random
from .. import loader, utils
from asyncio  import sleep
from random import randrange     

@loader.tds
class Inomte (loader.Module):
    strings = {"name": "Inomte",}

    async def client_ready(self, client, db)  -> None:
        self.db = db
        self.client = client

    async def Inomtecmd(self, message):
        """Пример ввода: .inomte <задержка появления текста в секундах> <текст>"""
        args = utils.get_args(message)
